    <Compile Remove="Hazelcast.Client.Protocol.Codec\BuiltIn\CodecUtil.cs" />
    <Compile Remove="Hazelcast.Client.Protocol.Codec\BuiltIn\DataCodec.cs" />
    <Compile Remove="Hazelcast.Client.Protocol.Codec\BuiltIn\FixedSizeTypesCodec.cs" />
    <Compile Remove="Hazelcast.Client.Protocol.Codec\BuiltIn\StringCodec.cs" />
  </ItemGroup>
  <ItemGroup>
