{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [ "Hazelcast.Net/Hazelcast.Net.csproj" ],
          "exclude": [ "**/obj/**", "**/bin/**" ]
        }
      ],
      "dest": "obj/dev/api",
      "disableGitFeatures": true,
      "disableDefaultFilter": false,
      "properties": { "TargetFramework":  "netstandard2.0" }
    }
  ],
  "build": {
    "content": [
      {
        "src": "obj/dev/api",
        "dest": "dev/api",
        "files": [ "**/*.yml" ]
      },
      {
        "files": [ "*.md", "toc.yml", "dev/api/index.md", "dev/toc.yml", "dev/doc/**.md" ]
      }
    ],
    "resource": [ { "files": [ "images/**" ] } ],
    "overwrite": [ { "files": [ "apidoc/*.md" ] } ],
    "globalMetadata": {
      "_appTitle": "Hazelcast .NET Documentation",
      "_appFooter": "Copyright &copy; 2010-2020 Hazelcast, Inc. All rights reserved.<br />Generated by DocFX.",
      "_appFaviconPath": "images/favicon.png",
      "_enableSearch": true,
      "_disableContribution": true
    },
    "dest": "../temp/docfx.site",
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "disableGitFeatures": true
  }
}
