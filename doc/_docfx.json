{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": [ "Hazelcast.Net/Hazelcast.Net.csproj" ],
          "exclude": [ "**/obj/**", "**/bin/**" ]
        }
      ],
      "dest": "obj/dev/api",
      "disableGitFeatures": true,
      "disableDefaultFilter": false,
      "properties": { "TargetFramework":  "netstandard2.0" }
    }
  ],
  "build": {
    "content": [
      {
        "src": "obj/dev/api",
        "dest": "__DEST__/api",
        "files": [ "**/*.yml" ]
      },
      {
        "files": [ "*.md", "toc.yml" ]
      },
      {
        "src": "dev",
        "dest": "__DEST__",
        "files": [ "api/index.md", "toc.yml", "doc/**.md", "cover/index.md" ]
      }
    ],
    "resource": [ { "files": [ "images/**" ] } ],
    "overwrite": [ { "files": [ "apidoc/*.md" ] } ],
    "globalMetadata": {
      "_appTitle": "Hazelcast .NET Documentation",
      "_appFooter": "Copyright &copy; 2010-2021 Hazelcast, Inc. All rights reserved.<br />Generated by DocFX.",
      "_appFaviconPath": "images/favicon.ico",
      "_enableSearch": true,
      "_disableContribution": true
    },
    "fileMetadata": {
      "build-options": {
        "dev/doc/configuration/options.md": true
      }
    },
    "dest": "../temp/docfx.out",
    "xrefService": [ "https://xref.docs.microsoft.com/query?uid={uid}" ],
    "xref": [
      "xrefmap.yml"
    ],
    "disableGitFeatures": true
  }
}
